<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Unit Test TestNg ">
    <test name="Groups Tests">
        <groups>
            <define name="All">
                <include name="Cart"/>
                <include name="Item"/>
                <include name="Parser"/>
            </define>
            <run>
                <include name="All"/>
                <exclude name="Cart"/>
            </run>
        </groups>

        <classes>
            <class name="unitTests.TestForClassCart"/>
            <class name="unitTests.TestForClassRealItem"/>
            <class name="unitTests.TestForClassVirtualItem"/>
            <class name="unitTests.TestsForClassJsonParser"/>
        </classes>
    </test>
</suite>